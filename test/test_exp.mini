# Test: Compare a simple exponential ODE with its analytic result

import default_logging.ini
import default_time_stepping.ini

_grow_rate = 0.2
_initial = 1
_geometry_type = cubes, triangles | expand geometry_type
_proc = 1, 2, 4 | expand processes

# Only test trianges in parallel
{_geometry_type} == cubes and {_proc} > 1 | exclude

__name = test_exp_{_geometry_type}_{_proc}

[wrapper.execute_parallel]
numprocesses = {_proc}

[grid]
dimension = 2
file = grids/square_{_geometry_type}.msh
initial_level = 0

[model]
order = 1

[model.time_stepping]
begin = 0
end = 10
initial_step = 1e-2
min_step = 1e-3
max_step = 1e-1

[model.writer]
file_path = {__name}

[model.compartments]
domain     = 0

############################ Model for domain ############################
[model.domain.initial]
u = {_initial}

[model.domain.reaction]
u = {_grow_rate}*u

[model.domain.reaction.jacobian]
du_du = {_grow_rate}

[model.domain.diffusion]
u = 0.

#### compare against a known solution
[model.domain.compare.expression]
u = exp({_grow_rate}*t)
[model.domain.compare.l2_error]
u = 2.4e-4
