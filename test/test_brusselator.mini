# Test: Simulation is run successfully for challenging timestepping setup

import default_logging.ini
import default_time_stepping.ini

__name = test_brusselator

[grid]
file = grids/square_triangles.msh
initial_level = 0
dimension = 2

[model]
order = 1

[model.time_stepping]
begin = 0
end = 100
initial_step = 0.1
min_step = 1e-10
max_step = 10000

[model.writer]
file_path = {__name}

[model.compartments]
compartment = 0

[model.compartment.initial]
X = 1
Y = 1

[model.compartment.reaction]
X = 0.5 - 4*X + X^2*Y
Y = 3*X - X^2*Y

[model.compartment.reaction.jacobian]
dX__dX = -4 + 2*X*Y
dX__dY = X^2
dY__dX = 3 - 2*X*Y
dY__dY = -X^2

[model.compartment.diffusion]
X = 0.2
Y = 0.5
